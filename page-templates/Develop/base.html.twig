{# base.html.twig #}
<!DOCTYPE html>
<!--[if IE 7]><html class="ie ie7" lang="{{ Wordpress.bloginfo('language') }}"><![endif]-->
<!--[if IE 8]><html class="ie ie8" lang="{{ Wordpress.bloginfo('language') }}"><![endif]-->
<!--[if !(IE 7) | !(IE 8)  ]><!--><html lang="{{ Wordpress.bloginfo('language') }}"><!--<![endif]-->
<head>
	<meta charset="{{ Wordpress.bloginfo('charset') }}" />
	<meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1, user-scalable=no" />
	<title>{{ Wordpress.bloginfo('title') }}</title>
	
    <link rel="icon" href="{{ Wordpress.getPath('Img') }}favicon.png" type="image/x-icon" />
	<link rel="icon" type="image/png" href="{{ Wordpress.getPath('Img') }}favicons/favicon-32x32.png" sizes="32x32" />
	<link rel="icon" type="image/png" href="{{ Wordpress.getPath('Img') }}favicons/favicon-96x96.png" sizes="96x96" />
	<link rel="icon" type="image/png" href="{{ Wordpress.getPath('Img') }}favicons/favicon-16x16.png" sizes="16x16" />
	<link rel="apple-touch-icon" sizes="57x57" href="{{ Wordpress.getPath('Img') }}favicons/apple-touch-icon-57x57.png" />
	<link rel="apple-touch-icon" sizes="60x60" href="{{ Wordpress.getPath('Img') }}favicons/apple-touch-icon-60x60.png" />
	<link rel="apple-touch-icon" sizes="72x72" href="{{ Wordpress.getPath('Img') }}favicons/apple-touch-icon-72x72.png" />
	<link rel="apple-touch-icon" sizes="76x76" href="{{ Wordpress.getPath('Img') }}favicons/apple-touch-icon-76x76.png" />
	<link rel="apple-touch-icon" sizes="114x114" href="{{ Wordpress.getPath('Img') }}favicons/apple-touch-icon-114x114.png" />
	<link rel="apple-touch-icon" sizes="120x120" href="{{ Wordpress.getPath('Img') }}favicons/apple-touch-icon-120x120.png" />
	<link rel="apple-touch-icon" sizes="144x144" href="{{ Wordpress.getPath('Img') }}favicons/apple-touch-icon-144x144.png" />
	<link rel="apple-touch-icon" sizes="152x152" href="{{ Wordpress.getPath('Img') }}favicons/apple-touch-icon-152x152.png" />
	<link rel="apple-touch-icon" sizes="180x180" href="{{ Wordpress.getPath('Img') }}favicons/apple-touch-icon-180x180.png" />
	<link rel="icon" type="image/png" href="{{ Wordpress.getPath('Img') }}favicons/android-chrome-192x192.png" sizes="192x192" />

	<link rel="manifest" href="{{ Wordpress.getPath('Img') }}favicons/manifest.json" />
	<meta name="msapplication-TileColor" content="#da532c" />
	<meta name="msapplication-TileImage" content="{{ Wordpress.getPath('Img') }}favicons/mstile-144x144.png" />
	<meta name="theme-color" content="#ffffff" />

	<link rel="profile" href="http://gmpg.org/xfn/11" />
	<!--[if lt IE 9]><script src="{{ Wordpress.getPath('Js') }}html5.js" type="text/javascript"></script><![endif]-->
    <script src="{{ Wordpress.getPath('Js') }}libs/modernizr-2.6.2.min.js" type="text/javascript"></script>

    <link href="//maxcdn.bootstrapcdn.com/bootstrap/3.3.4/css/bootstrap.min.css" rel="stylesheet">

	{{ Wordpress.wp_head }}
</head>
<body {{ Wordpress.body_class('responsive') }} data-spy="scroll">
    <script lang="text/javascript">
		var _host = '{{ Wordpress.home_url() }}';
	</script>

	<div class="container-fluid">
		<!-- block layout -->
		{% block layout %}
		{% endblock layout %}
		<!-- /block layout -->
	</div>

	<script src="https://ajax.googleapis.com/ajax/libs/jquery/2.1.4/jquery.min.js"></script>
	<script src="//maxcdn.bootstrapcdn.com/bootstrap/3.3.4/js/bootstrap.min.js"></script>
	<script src="//cdnjs.cloudflare.com/ajax/libs/ScrollMagic/2.0.2/ScrollMagic.min.js"></script>
	<script src="//cdnjs.cloudflare.com/ajax/libs/ScrollMagic/2.0.2/plugins/jquery.ScrollMagic.min.js"></script>
	<script src="//cdnjs.cloudflare.com/ajax/libs/ScrollMagic/2.0.2/plugins/debug.addIndicators.min.js"></script>
	<script src="http://cdnjs.cloudflare.com/ajax/libs/gsap/latest/TweenMax.min.js"></script>
	<script src="http://cdnjs.cloudflare.com/ajax/libs/gsap/latest/plugins/ScrollToPlugin.min.js"></script>

	{{ Wordpress.wp_footer }}
	<script type="text/javascript" src="{{ Wordpress.getPath('Js') }}dev.js"></script>
</body>
</html>